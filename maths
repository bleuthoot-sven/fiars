39 / 7

32 % 7 = 4

36 % 8 = 4 (*8=32)
37 % 8 = 5
38 % 8 = 6
39 % 8 = 7


36 - 8 = 28

39 -> 33 -> 27 -> done
   -6    -6
39 - (6 * 2) = 27
39 / 6 = 6
39 % 6 = 3

38 -> 32 -> 26 -> 20 -> done
   -6    -6    -6
38 - (6 * 3) = 20
38 / 6 = 6

35 % 7 = 0
35 / 7 = 5
0 + 5 = 5
5 - 6 = -1 -> 0 (saturated overflow)
5 + (0 * WIDTH) = 5

38 % 7 = 3
38 / 7 = 5
3 + 5 = 8
8 - 6 = 2
8 + (2 * WIDTH) = 20

39 % 7 = 4
39 / 7 = 5
4 + 5 = 9
9 - 6 = 3
9 + (3 * 6) = 27

